<?xml version="1.0" encoding="UTF-8"?>
<skin version="0.2">
    <title xml:lang="ko">The Bootstrap 자료실</title>
    <title xml:lang="en">The Bootstrap Resource skin</title>
    <description xml:lang="ko">The Bootstrap 테마 용 자료실 스킨입니다.</description>
    <description xml:lang="en">Resource module skin for The Bootstrap skin</description>
    <version>0.1</version>
    <date>2012-07-19</date>
    <author email_address="wincomi@me.com" link="http://xe.pe.kr">
        <name xml:lang="ko">윈컴이</name>
        <name xml:lang="en">wincomi</name>
        <name xml:lang="zh-TW">wincomi</name>
		<name xml:lang="vi">wincomi</name>
